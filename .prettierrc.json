{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": false,
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 300,
    "proseWrap": "preserve",
    "arrowParens": "always",
    "vueIndentScriptAndStyle": true,
    "singleAttributePerLine": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "embeddedLanguageFormatting": "auto",
    "bracketSameLine": true
}
